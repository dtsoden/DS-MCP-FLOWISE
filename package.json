{
  "name": "ds-mcp-flowise",
  "version": "1.2.10",
  "description": "MCP server for building and managing Flowise chatflows and agentflows. Provides AI assistants with comprehensive knowledge of Flowise nodes, tools for workflow creation, and direct API integration to deploy chatflows.",
  "main": "dist/index.js",
  "type": "module",
  "files": [
    "dist",
    "data",
    "scripts",
    "src",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "ds-mcp-flowise": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "extract": "tsx scripts/extract-nodes.ts",
    "prepare-db": "tsx scripts/prepare-database.ts"
  },
  "keywords": [
    "mcp",
    "flowise",
    "chatflow",
    "agentflow",
    "ai",
    "workflow",
    "langchain",
    "claude",
    "model-context-protocol"
  ],
  "author": "David Soden",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "better-sqlite3": "^12.6.2",
    "glob": "^10.3.0",
    "sql.js": "^1.10.0",
    "typescript": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/sql.js": "^1.4.9",
    "tsx": "^4.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dtsoden/DS-MCP-FLOWISE.git"
  },
  "homepage": "https://github.com/dtsoden/DS-MCP-FLOWISE#readme",
  "bugs": {
    "url": "https://github.com/dtsoden/DS-MCP-FLOWISE/issues"
  }
}
